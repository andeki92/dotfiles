# Minimal aliases file
# Only includes the most essential aliases

# Navigation
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'

# General
alias c='clear'
alias h='history'
alias rl='typeset -U path && source ~/.zshrc'

# Listing
alias l='ls -lah'
alias la='ls -lAh'
alias ll='ls -lh'
